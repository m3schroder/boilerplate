{
	"files.exclude": {
		".github": true,
		".vercel": true,
		"**/node_modules": true,
		"encore": true,
		"**/encore.gen*": true,
		".encore*": true,
		"**/README.md": false,
		"**/.svelte-kit": true,
		"**/build": true,
		"**/dist": true,
		"**/*prettier*": true,
	},
	"editor.codeActionsOnSave": {
		"source.organizeImports": true,
	},
	"editor.tabSize": 5,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
		"all.config": "bun.lockb,package.json,.env*,.git*,go.*, *ts, .*,*js, tsconfig.json, docker*, README.md"
	},
}